{
  "name": "pgp-generator",
  "version": "1.0.0",
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:macos": "react-scripts build && electron-builder --macos",
    "electron-start": "electron ."
  },
  "dependencies": {
    "openpgp": "4.8.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.0",
    "styled-components": "5.0.0"
  },
  "devDependencies": {
    "@types/openpgp": "4.4.8",
    "@types/react": "16.9.17",
    "@types/styled-components": "4.4.2",
    "electron": "15.5.5",
    "electron-builder": "22.2.0",
    "typescript": "3.7.5"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
